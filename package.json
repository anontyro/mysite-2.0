    "apollo-link": "^1.2.8",
    "apollo-link-http": "^1.5.11",
    "apollo-server-express": "^2.4.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "graphql": "^14.1.1",
    "graphql-upload": "^8.0.4",
    "isomorphic-unfetch": "^3.0.0",
    "jsonwebtoken": "^8.4.0",
    "lodash.get": "^4.4.2",
    "lodash.throttle": "^4.1.1",
