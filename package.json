    "@types/styled-jsx": "^2.2.8",
    "@types/zeit__next-typescript": "^0.1.1",
    "ava": "^1.2.1",
    "nodemon": "^1.18.10",
    "ts-node": "^8.0.2",
    "typescript": "3.2.4"
  },
  "license": "MIT",
