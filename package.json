{
  "name": "my-site-nextjs-express-graphql",
  "version": "3.5.0",
  "engines": {
    "node": "14.4.0"
  },
  "scripts": {
    "dev:server": "nodemon",
    "dev": "nodemon -w server/ -w server.tsx --exec yarn type-check",
    "clean": "rm -rf dist && mkdir dist",
    "docker:build": "npm run clean && npm run build && tsc --project tsconfig.server.json",
    "prod:server": "NODE_ENV=production node ./dist/server.jsx",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc && ts-node dist/server.js",
    "test": "tsc -m commonjs && ava",
    "test:watch": "chokidar \"./**/*.test.*\" -c \"npm run test\" --initial",
    "check-outdated": "check-outdated --ignore-pre-releases --ignore-dev-dependencies --ignore-packages package1,package2 --columns name,type,current,latest,changes"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-brands-svg-icons": "^5.14.0",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "@mdx-js/loader": "^1.6.18",
    "@next/mdx": "^9.5.3",
    "@types/node-fetch": "^2.5.7",
    "@zeit/next-typescript": "^1.1.1",
    "apollo-boost": "^0.4.9",
    "apollo-link": "^1.2.14",
    "apollo-link-http": "^1.5.17",
    "apollo-server-express": "^2.18.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^14.5.8",
    "graphql-upload": "^11.0.0",
    "isomorphic-unfetch": "^3.0.0",
    "jsonwebtoken": "^8.5.1",
    "lodash.get": "^4.4.2",
    "lodash.throttle": "^4.1.1",
    "moment": "^2.29.0",
    "mysql": "^2.18.1",
    "next": "9.5.3",
    "next-apollo": "^5.0.0",
    "next-redux-wrapper": "^6.0.2",
    "punkapi-lib": "^2.5.0",
    "react": "16.13.1",
    "react-apollo": "^3.1.5",
    "react-dom": "16.13.1",
    "react-quill": "^1.3.5",
    "react-redux": "^7.2.1",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "reflect-metadata": "^0.1.13",
    "sinon": "^9.0.3",
    "tslint": "^6.1.3",
    "type-graphql": "^0.17.6",
    "typeorm": "^0.2.26"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.8",
    "@types/graphql": "^14.5.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/next": "^9.0.0",
    "@types/node": "^14.11.2",
    "@types/react": "^16.9.49",
    "@types/react-dom": "16.9.8",
    "@types/sinon": "^9.0.6",
    "@types/styled-jsx": "^2.2.8",
    "@types/zeit__next-typescript": "^0.1.4",
    "ava": "^3.13.0",
    "check-outdated": "^2.4.0",
    "chokidar": "^3.4.2",
    "nodemon": "^2.0.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "license": "MIT",
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register/transpile-only"
    ]
  }
}