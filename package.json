  "scripts": {
    "dev": "nodemon -w server/ -w server.tsx --exec yarn type-check",
    "clean": "rm -rf dist && mkdir dist",
    "docker:build": "npm run clean && npm run build && tsc ",
    "prod:server": "node ./dist/server.js",
